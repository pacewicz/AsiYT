<div id="wrapper" class="single-container">
  <div class="container">
    <div class="row">
      <h3>Users</h3>
      <div class="column">
        <table class="table">
          <tbody>
          <% @users.each do |user| %>
              <tr>
                <td><%= user.nick %></td>
                <td><%= link_to user.email, user %></td>
              </tr>
          <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>